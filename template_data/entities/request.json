            "type": {
                "name": "relationship",
                "arguments": {
                    "linked": {
                        "class": "User",
                        "attribute": "requests",
                        "attribute_presented": "name"
                    },
                    "cardinality": "one"
                }
            },
            "validations": [
                "required"
            ]
        }, {
            "name": "request_type",
            "label": "Tipo de Solicitud",
            "type": {
                "name": "relationship",
                "arguments": {
                    "linked": {
                        "class": "RequestType",
                        "attribute": "requests",
                        "attribute_presented": "name"
                    },
                    "cardinality": "one"
                }
            },
            "validations": [
                "required"
            ]
        }
    ]
