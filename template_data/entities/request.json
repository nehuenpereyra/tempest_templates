                "name": "string",
                "arguments": {
                    "length": 200,
                    "is_large": true
                }
            },
            "validations": [
                "required",
                "length-3-200"
            ],
            "searchable": false,
            "is_main": true
        }, {
            "name": "is_resolved",
            "label": "Resuelto",
            "type": {
                "name": "boolean"
            },
            "default": false,
            "searchable": true
        }, {
            "name": "receive_email",
            "label": "Recibir Correo Electrónico",
            "type": {
                "name": "boolean"
            },
            "default": false,
            "searchable": false
        }, {
            "name": "timestamp",
            "label": "Fecha de Creación",
            "type": {
                "name": "datetime"
            },
            "validations": [
                "required"
            ],
            "searchable": true
        }, {
            "name": "user",
