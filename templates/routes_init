
from . import main
from . import auth
from . import setting
from . import handler
[% for module in modules %]
from . import [[ module ]]
[% endfor %]


def set_routes(app):
    
    main.set_routes(app)
    auth.set_routes(app)
    setting.set_routes(app)
    handler.set_routes(app)
[% for module in modules %]
    [[ module ]].set_routes(app)
[% endfor %]
